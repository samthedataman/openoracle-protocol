{
  "name": "@openoracle/react",
  "version": "1.0.0",
  "description": "React SDK for OpenOracle prediction markets and oracle data integration",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "build:types": "tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "oracle",
    "prediction-markets",
    "blockchain",
    "defi",
    "chainlink",
    "pyth",
    "typescript"
  ],
  "author": "OpenOracle Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/openoracle/react-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/openoracle/react-sdk/issues"
  },
  "homepage": "https://github.com/openoracle/react-sdk#readme",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.28.0",
    "zustand": "^4.5.2",
    "ethers": "^6.11.0",
    "@wagmi/core": "^2.6.0",
    "wagmi": "^2.5.0",
    "viem": "^2.9.0",
    "axios": "^1.6.0",
    "socket.io-client": "^4.7.0",
    "decimal.js": "^10.4.0",
    "date-fns": "^3.6.0",
    "immer": "^10.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@testing-library/react": "^14.2.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/user-event": "^14.5.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3",
    "rollup-plugin-visualizer": "^5.12.0"
  },
  "publishConfig": {
    "access": "public"
  }
}