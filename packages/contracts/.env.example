# Private key for deployment (DO NOT COMMIT ACTUAL KEY)
PRIVATE_KEY=your_private_key_here

# Contract address after deployment (will be filled after deploy)
CONTRACT_ADDRESS=

# Fee recipient address (treasury) - can be same as deployer
FEE_RECIPIENT=0x...

# Base Network RPC URLs (optional - defaults to public endpoints)
# You can get free RPC endpoints from:
# - Alchemy: https://www.alchemy.com/
# - Infura: https://infura.io/
# - QuickNode: https://www.quicknode.com/
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org
BASE_MAINNET_RPC_URL=https://mainnet.base.org

# Basescan API key for contract verification
# Get it from: https://basescan.org/apis
BASESCAN_API_KEY=

